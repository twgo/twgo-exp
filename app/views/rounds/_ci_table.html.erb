<script>
function clip_cmd(e) {
  var x = document.createElement("INPUT");
  x.setAttribute("type", "text");
  x.setAttribute("type", "hidden");
  x.value = e.title;
  x.select();
  document.execCommand("Copy");
  alert(x.value);
  x.remove();
}
</script>
